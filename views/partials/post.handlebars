<article class="post">
  <div class="image-content">
    <img class="post-image" src="{{"postURL"}}" />
  </div>
  <div class="post-content">
    <p class="post-reply">
       <a href="/posts/{{"postReply"}}"> >>{{"postReply"}} </a>
    </p>
    <p class="post-text">
      {{"postText"}}
    </p>
    <p class="post-author">
      <a href="#">{{"postAuthor"}}</a>
    </p>
	<p class="post-ID">
	  {{"postID"}}
	</p>
  </div>
</article>
